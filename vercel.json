{
    "version": 2,
    "build": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node",
            "config": {
        "includeFiles": [
            "prisma/schema.prisma",
            "node_modules/.prisma/client/**"
        ]
    }
        }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest":"src/main.ts",
            "method": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}